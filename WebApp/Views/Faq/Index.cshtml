@{
}
@model IEnumerable<WebApp.Models.Faq>

<h2>常見問答</h2>
<ul>
    @foreach (var faq in Model)
    {
        <li>
            <b>Q: @faq.Question</b><br />
            <span>A: @faq.Answer</span>
        </li>
    }
</ul>